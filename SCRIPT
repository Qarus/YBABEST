local library = loadstring(game:HttpGet("https://pastebin.com/raw/LHQ5vsy9"))();


local Gamer = library:CreateSection("Yba");
Gamer:Button("~ Press Right Shift To Hide GUI ~");
function LoadMe(link)
    loadstring(game:HttpGet(link))()
end
Gamer:Button("Blue Script (Best)", function()
    loadstring(game:HttpGet"https://raw.githubusercontent.com/NukeVsCity/hackscript123/main/gui")()
end)
Gamer:Button("BLACK GUI", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Tobias020108Back/YBA/main/Yba/All/In/One/NonAutoExec"))()
end)

library:Ready();
